<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.co.lookst.sns.dao.snsMapper">

   <select id="profileInfo" resultType="SnsProfileDto">
      select * from profileInfo where member_id = #{member_id}
   </select>
   
   <select id="profileFeed" resultType="ProfileFeedDto">
      select * from profileFeed where member_id = #{member_id}
   </select>
   
<!--    <select id="feedCnt" resultType="ProfileFeedDto"> -->
<!--       select * from feed_cnt  -->
<!--    </select> -->
   
   <select id="followerList" resultType="FollowDto">
      select * from follower_list where member_id = #{member_id}
   </select>
   
   <select id="followingList" resultType="FollowDto">
      select * from following_list where member_id = #{member_id}
   </select>
</mapper>